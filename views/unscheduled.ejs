<%- include('partials/header', { active: 'unscheduled' }) %>

 <div class="p-[32px]">
      <div class="flex flex-wrap items-end gap-[4px] py-[8px] px-[16px] bg-[#ffffff] border border-[#e2e8f0] rounded-[12px] mb-[16px]">
        <div class="min-w-[100px] flex flex-col gap-[2px] shrink basis-auto grow-0" id="filterDate"></div>
      </div>

      <!-- Tabs -->
      <div class="tabs-container">
        <button class="tab-button active" data-tab="classes">
          Classes (<span id="classesCount">0</span>)
        </button>
        <button class="tab-button" data-tab="faculties">
          Faculties (<span id="facultiesCount">0</span>)
        </button>
        <button class="tab-button" data-tab="rooms">
          Rooms (<span id="roomsCount">0</span>)
        </button>
      </div>

      <div class="tab-content active" id="classesTab">
        <div id="classesContainer" class="unscheduled-list"></div>
      </div>

      <div class="tab-content" id="facultiesTab">
        <div id="facultiesContainer" class="unscheduled-list"></div>
      </div>

      <div class="tab-content" id="roomsTab">
        <div id="roomsContainer" class="unscheduled-list"></div>
      </div>

      <div id="loadingState" class="loading-state" style="display: none;">
        <div class="spinner"></div>
        <p>Loading unscheduled items...</p>
      </div>

      <div id="emptyState" class="empty-state" style="display: none;">
        <div class="empty-icon">âœ…</div>
        <h3>Everything is scheduled!</h3>
        <p>No unscheduled items found for this date</p>
      </div>

</div>

    </div>
  </div>

  <script src="/js/unscheduled.js"></script>
</body>
</html>
